{
  "trader_id": "NAUTILUS-SANDBOX-{instance_id}",
  "log_level": "{log_level}",
  "load_state": false,
  "save_state": false,
  "loop_debug": true,
  "cache": {
    "type": "redis",
    "host": "{redis_host}",
    "port": {redis_port},
    "db": {redis_db},
    "flush": true
  },
  "message_bus": {
    "database": {
      "type": "redis",
      "host": "{redis_host}",
      "port": {redis_port},
      "db": {redis_db}
    },
    "encoding": "msgpack",
    "timestamps_as_iso8601": true,
    "buffer_interval_ms": 0
  },
  "data_engine": {
    "qsize": 10000,
    "time_bars_build_with_no_updates": true,
    "time_bars_timestamp_on_close": false,
    "validate_data_sequence": false
  },
  "risk_engine": {
    "bypass": true,
    "max_order_rate": "1000/00:00:01",
    "max_notionals_per_order": {}
  },
  "exec_engine": {
    "load_cache": false,
    "qsize": 10000
  },
  "streaming": {
    "catalog_path": "{catalog_path}",
    "fs_protocol": "file",
    "fs_storage_options": null
  },
  "strategies": [],
  "adapters": []
}