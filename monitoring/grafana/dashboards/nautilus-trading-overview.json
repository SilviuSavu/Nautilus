{
  "dashboard": {
    "id": null,
    "title": "Nautilus Trading Platform - Overview",
    "description": "Comprehensive trading platform monitoring dashboard for Sprint 3",
    "tags": ["nautilus", "trading", "overview"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Trading Performance Summary",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            },
            "unit": "currencyUSD"
          },
          "overrides": []
        },
        "targets": [
          {
            "expr": "nautilus:portfolio_pnl_total",
            "refId": "A",
            "legendFormat": "Total P&L"
          },
          {
            "expr": "rate(nautilus_trades_total[1h]) * 3600",
            "refId": "B",
            "legendFormat": "Trades/Hour"
          },
          {
            "expr": "nautilus:portfolio_var_95",
            "refId": "C",
            "legendFormat": "VaR 95%"
          }
        ]
      },
      {
        "id": 2,
        "title": "WebSocket Connections",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "nautilus:websocket_connections_active",
            "refId": "A",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "nautilus:websocket_messages_per_second",
            "refId": "B",
            "legendFormat": "Messages/sec"
          }
        ]
      },
      {
        "id": 3,
        "title": "Portfolio Performance",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "USD",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "currencyUSD"
          }
        },
        "targets": [
          {
            "expr": "nautilus_portfolio_total_value",
            "refId": "A",
            "legendFormat": "Total Portfolio Value"
          },
          {
            "expr": "nautilus_portfolio_unrealized_pnl",
            "refId": "B",
            "legendFormat": "Unrealized P&L"
          },
          {
            "expr": "nautilus_portfolio_realized_pnl",
            "refId": "C",
            "legendFormat": "Realized P&L"
          }
        ]
      },
      {
        "id": 4,
        "title": "Risk Metrics",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": -10000},
                {"color": "red", "value": -50000}
              ]
            },
            "unit": "currencyUSD",
            "min": -100000,
            "max": 0
          }
        },
        "targets": [
          {
            "expr": "nautilus_risk_var_amount{confidence_level=\"0.95\"}",
            "refId": "A",
            "legendFormat": "VaR 95% (1d)"
          }
        ]
      },
      {
        "id": 5,
        "title": "Trade Execution Quality",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "Basis Points",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "avg(nautilus_execution_slippage_bps)",
            "refId": "A",
            "legendFormat": "Average Slippage (bps)"
          },
          {
            "expr": "avg(nautilus_execution_time_ms)",
            "refId": "B",
            "legendFormat": "Avg Execution Time (ms)"
          }
        ]
      },
      {
        "id": 6,
        "title": "System Health",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 25},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "filterable": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "up{job=\"nautilus-backend\"}",
            "refId": "A",
            "legendFormat": "Backend Status",
            "format": "table",
            "instant": true
          },
          {
            "expr": "nautilus:database_connection_utilization",
            "refId": "B",
            "legendFormat": "DB Connection %",
            "format": "table",
            "instant": true
          },
          {
            "expr": "rate(nautilus_http_requests_total[5m])",
            "refId": "C",
            "legendFormat": "API Requests/sec",
            "format": "table",
            "instant": true
          }
        ]
      },
      {
        "id": 7,
        "title": "Strategy Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 25},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "Return %",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "nautilus:strategy_performance_total",
            "refId": "A",
            "legendFormat": "{{strategy_id}}"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "portfolio",
          "type": "query",
          "query": "label_values(nautilus_portfolio_total_value, portfolio_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "current": {"value": ["$__all"], "text": "All"},
          "options": []
        },
        {
          "name": "strategy",
          "type": "query", 
          "query": "label_values(nautilus_strategy_pnl_dollars, strategy_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "current": {"value": ["$__all"], "text": "All"},
          "options": []
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Risk Breaches",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "increase(nautilus_risk_breaches_total[1m]) > 0",
          "iconColor": "red",
          "titleFormat": "Risk Breach: {{severity}}",
          "textFormat": "{{description}}"
        },
        {
          "name": "Engine Events",
          "datasource": "Prometheus", 
          "enable": true,
          "expr": "changes(nautilus_engine_status[5m]) > 0",
          "iconColor": "blue",
          "titleFormat": "Engine Status: {{status}}",
          "textFormat": "Engine {{instance}} changed status"
        }
      ]
    },
    "links": [
      {
        "title": "Risk Dashboard",
        "url": "/d/nautilus-risk/risk-management",
        "type": "dashboards"
      },
      {
        "title": "System Monitoring",
        "url": "/d/nautilus-system/system-monitoring", 
        "type": "dashboards"
      },
      {
        "title": "Real-time Monitoring",
        "url": "/d/nautilus-realtime/realtime-monitoring",
        "type": "dashboards"
      }
    ]
  }
}