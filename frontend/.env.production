# Production Environment Configuration
# Nautilus Trading Platform - Frontend Integration

# API Configuration
VITE_API_BASE_URL=http://localhost:8001
VITE_WS_URL=ws://localhost:8001

# Engine Endpoints (Production)
VITE_ANALYTICS_ENGINE=http://localhost:8100
VITE_RISK_ENGINE=http://localhost:8200
VITE_FACTOR_ENGINE=http://localhost:8300
VITE_ML_ENGINE=http://localhost:8400
VITE_FEATURES_ENGINE=http://localhost:8500
VITE_WEBSOCKET_ENGINE=http://localhost:8600
VITE_STRATEGY_ENGINE=http://localhost:8700
VITE_MARKETDATA_ENGINE=http://localhost:8800
VITE_PORTFOLIO_ENGINE=http://localhost:8900

# M4 Max Hardware Acceleration
VITE_M4_MAX_OPTIMIZED=true
VITE_METAL_ACCELERATION=true
VITE_NEURAL_ENGINE_ENABLED=true

# Performance Settings
VITE_ENABLE_WEBSOCKET_STREAMING=true
VITE_AUTO_REFRESH_INTERVAL=5000
VITE_API_TIMEOUT=10000
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=10

# Feature Flags
VITE_ENABLE_VOLATILITY_ENGINE=true
VITE_ENABLE_ENHANCED_RISK=true
VITE_ENABLE_M4MAX_MONITORING=true
VITE_ENABLE_MULTI_ENGINE_HEALTH=true

# Debug and Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_LOG_LEVEL=info
VITE_ENABLE_ERROR_REPORTING=true